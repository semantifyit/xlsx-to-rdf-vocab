{
    "name": "xlsx-to-rdf-vocab",
    "version": "1.0.0",
    "description": "converts xlsx/google sheets to rdf vocabularies",
    "main": "dist/index.js",
    "bin": "dist/cli.js",
    "browser": "dist/bundle.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "build-browser": "webpack",
        "build-all": "npm run build && npm run build-browser",
        "test": "jest"
    },
    "keywords": [
        "xsls",
        "rdf",
        "schema.org",
        "json-ld"
    ],
    "author": "Thibault Gerrier",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^25.1.3",
        "jest": "^25.1.0",
        "prettier": "^1.19.1",
        "ts-jest": "^25.2.1",
        "ts-loader": "^6.2.1",
        "typescript": "^3.8.2",
        "webpack": "^4.41.6",
        "webpack-cli": "^3.3.11"
    },
    "dependencies": {
        "node-fetch": "^2.6.0",
        "xlsx": "^0.17.0"
    },
    "prettier": {
        "singleQuote": true,
        "trailingComma": "all",
        "arrowParens": "always",
        "tabWidth": 2,
        "printWidth": 110
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node"
    }
}
